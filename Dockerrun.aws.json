{
  "AWSEBDockerrunVersion": 2,
  "authentication": {
    "bucket": "elasticbeanstalk-us-east-1-971716655829",
    "key": "dockercfg.taskmuncher/.dockercfg"
  },

  "containerDefinitions": [
    {
      "name": "streamlitui",
      "image": "153432822124.dkr.ecr.ap-south-1.amazonaws.com/onboarding-automation-ui:latest",
      "hostname": "ui",
      "essential": false,
      "memory": 128
    },

    {
      "name": "nginx-proxy",
      "image": "nginx",
      "cpu": 0,
      "memory": 128,
      "links": ["php-app"],
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [
        {
          "sourceVolume": "php-app",
          "containerPath": "/var/www/html",
          "readOnly": true
        },
        {
          "sourceVolume": "awseb-logs-nginx-proxy",
          "containerPath": "/var/log/nginx"
        },
        {
          "sourceVolume": "nginx-proxy-conf",
          "containerPath": "/etc/nginx/conf.d",
          "readOnly": true
        }
      ],
      "volumesFrom": []
    }
  ],

  "volumes": [
    {
      "name": "streamlitui",
      "host": {
        "sourcePath": "/var/app/current/streamlitui"
      }
    },
    {
      "name": "awseb-logs-streamlitui",
      "host": {
        "sourcePath": "/var/log/containers/streamlitui"
      }
    },

    {
      "name": "nginx-proxy-conf",
      "host": {
        "sourcePath": "/var/app/current/proxy/conf.d"
      }
    },
    {
      "name": "awseb-logs-nginx-proxy",
      "host": {
        "sourcePath": "/var/log/containers/nginx-proxy"
      }
    }
  ],

